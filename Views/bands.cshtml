@{
  Layout="Shared/_Layout.cshtml";
}

<h2>Bands:</h2>
<hr />
@if (Model.Count != 0)
{
  <ul>
    @foreach (var band in Model)
    {
      <li><a href="/bands/@band.GetId()">@band.GetName()</a></li>
    }
  </ul>
}
<h4><a href="/bands/new">Add a new band</a></h4>
<p><a class="btn-sm btn-primary" href="/">Return to Main Page</a></p>
